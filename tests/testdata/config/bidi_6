# when condition satisfied using insertion order
[
    {
        "definition": {
            "baz": {
                "type": ["boolean"],
                "mapping": ["/foo/baz"]
            }
        }
    },
    {
        "when": { "bar": true },
        "definition": {
            "bar": {
                "type": ["string"],
                "mapping": ["/foo/bar"]
            }
        }
    }
]
{"baz":true,"bar":"test"}
{"foo":{"baz":true,"bar":"test"}}