# wildcard mapping
[
    {
        "definition":{
            "name":{
                "type":["string"],
                "mapping":["/fullname"]
            },
            "person":{
                "type":["boolean"],
                "mapping":[
                    {
                        "value":true, 
                        "template":{
                            "fullname":"[[string]]",
                            "entry":"person"
                        }
                    }
                ]
            }
        }
    }
]
{"name":"John Doe","person":true}
{"fullname":"John Doe","entry":"person"}