{
    "schema": {
        "reconfix": {
            "targets": {
                "file1": {
                    "type": "file",
                    "format": "json",
                    "location": {
                        "partition": 0,
                        "path": "/file1"
                    }
                }
            }
        },
        "properties": {
            "parent": {
                "reconfix": {
                    "transforms": [
                        {
                            "const": {
                                "properties": {
                                    "bar": { "const": "value1" }
                                }
                            },
                            "output": {
                                "target": "file1",
                                "path": "/foo"
                            }
                        }
                    ]
                },
                "properties": {
                    "child": {
                        "reconfix": {
                            "transforms": [
                                {
                                    "output": {
                                        "target": "file1",
                                        "path": "/foo/baz"
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        }
    },
    "dry": {
        "parent": {
            "child": "value2"
        }
    },
    "wet": {
        "/file1": {
            "foo": {
                "bar": "value1",
                "baz": "value2"
            }
        }
    }
}