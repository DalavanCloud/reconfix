{
    "schema": {
        "reconfix": {
            "targets": {
                "file1": {
                    "type": "file",
                    "format": "json",
                    "location": {
                        "partition": 0,
                        "path": "/file1"
                    }
                }
            }
        },
        "properties": {
            "value": {
                "reconfix": {
                    "transforms": [
                        {
                            "map": "identity",
                            "output": {
                                "target": "file1",
                                "path": "/foo/bar"
                            }
                        }
                    ]
                }
            },
            "type": {
                "reconfix": {
                    "transforms": [
                        {
                            "map": [
                                ["number", { "type": "number"}],
                                ["string", { "type": "string"}]
                            ],
                            "output": {
                                "target": "file1",
                                "path": "/foo/bar"
                            }
                        }
                    ]
                }
            }
        }
    },
    "dry": {
        "value": "test_value",
        "type": "string"
    },
    "wet": {
        "/file1": {
            "foo": {
                "bar": "test_value"
            }
        }
    }
}